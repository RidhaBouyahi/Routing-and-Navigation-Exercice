{
  "_version": "1.65.0",
  "sap.app": {
    "id": "demonr",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.4",
      "toolsId": "c3f00bfb-a09f-49db-97d3-038f9b932c05"
    },
    "dataSources": {
      "employeeRemote": {
        "uri": "/here/goes/your/serviceUrl/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.130.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "demonr.i18n.i18n"
        }
      },
      "": {
        "dataSource": "employeeRemote"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "demonr.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false,
        "bypassed": {
          "target": "notfound"
        }
      },
      "routes": [
        {
          "name": "appHome",
          "pattern": "",
          "target": [
            "home"
          ]
        },
        {
          "name": "EmployeeList",
          "pattern": "employees",
          "target": [
            "employees"
          ]
        },
        {
          "pattern": "employees/overview",
          "name": "employeeOverview",
          "target": ["EmployeeOverviewTop", "EmployeeOverviewContent"]
        },
        {
          "name": "EmployeeDetail",
          "pattern": "employees/{employeeId}",
          "target": [
            "detail"
          ]
        },
        {
          "name": "employeeResume",
          "pattern": "employees/{employeeId}/Resume:?query:",
          "target": [
            "resume"
          ]
        }
      ],
      "targets": {
        "home": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "home",
          "viewName": "Home",
          "viewLevel": 1
        },
        "notfound": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "notfound",
          "viewName": "NotFound",
          "viewLevel": 2
        },
        "employees": {
          "viewType": "XML",
          "transition": "slide",
          "viewPath": "demonr.view.employee",
          "viewId": "employeeList",
          "viewName": "EmployeeList",
          "viewLevel": 2
        },
        "detail": {
          "viewType": "XML",
          "transition": "slide",
          "viewPath": "demonr.view.employee",
          "viewId": "employeeDetail",
          "viewName": "Employee",
          "viewLevel": 3
        },
        "resume": {
          "viewType": "XML",
          "transition": "flip",
          "viewId": "resume",
          "viewName": "employee.Resume",
          "viewLevel": 4
        },
				"EmployeeOverview": {
					"viewId": "employeeOverview",
					"viewPath": "demonr.view.employee.overview",
					"viewName": "EmployeeOverview",
          "viewType": "XML",
          "transition":"slide",
					"level" : 2
				},
				"EmployeeOverviewTop": {
					"parent": "EmployeeOverview",
					"viewId": "employeeOverviewTop",
					"viewPath": "demonr.view.employee.overview",
					"viewName": "EmployeeOverviewTop",
					"controlId": "EmployeeOverviewParent",
					"controlAggregation": "content"
				},
				"EmployeeOverviewContent": {
					"parent": "EmployeeOverview",
					"viewId": "employeeOverviewContent",
					"viewPath": "demonr.view.employee.overview",
					"viewName": "EmployeeOverviewContent",
					"controlId": "EmployeeOverviewParent",
					"controlAggregation": "content"
				}
      }
    },
    "rootView": {
      "viewName": "demonr.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "demonr"
  }
}