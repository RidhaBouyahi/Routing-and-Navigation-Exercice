<mvc:XMLView
    controllerName="demonr.controller.NotFound"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    displayBlock="true"
>
    <Page>
        <content>
            <Button
                text="{i18n>BackButtonText}"
                icon="sap-icon://nav-back"
                class="sapUiSmallMarginEnd"
                press="onNavBack"
            />
            <IllustratedMessage
                title="{i18n>NotFound}"
                description="{i18n>NotFound.description}"
            />
        </content>
    </Page>
</mvc:XMLView>
